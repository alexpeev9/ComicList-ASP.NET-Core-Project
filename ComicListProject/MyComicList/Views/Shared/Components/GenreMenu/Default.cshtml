@foreach (var genre in Model)
{
    <a class="navbar-brand"
       asp-controller="Genre" asp-action="ListGenre"
       asp-route-genre="@genre.Name">@genre.Name <img src="@genre.Icon" class="img-fluid" alt=""></a>
}
<a class="navbar-brand" asp-controller="Genre" asp-action="ListGenre"> View all comics</a>